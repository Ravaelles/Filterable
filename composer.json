{
    "name": "ravaelles/filterable",
    "description": "Package for handling record filtering in Laravel 5.*",
    "keywords": [
        "laravel",
        "filterable",
		"filter",
		"records"
    ],
    "homepage": "https://github.com/Ravaelles/Filterable",
    "license": "MIT",
    "authors": [
        {
            "name": "Rafal Poniatowski",
            "email": "ravaelles@gmail.com",
            "homepage": "www.rafalponiatowski.tk",
            "role": "Web Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Ravaelles\\Filterable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ravaelles\\Filterable\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
